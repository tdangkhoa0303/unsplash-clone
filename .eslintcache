er._id];\n          } else {\n            likes = Array.from(new Set(photo.likes).delete(auth.user._id));\n          }\n          return {\n            ...photos,\n            data: {\n              ...photos.data,\n              [photoId]: {\n                ...photo,\n                likes,\n              },\n            },\n          };\n        });\n        await api.reactPhoto(photoId);\n      }\n    } catch (err) {\n      console.log(err);\n    }\n  };\n\n  const getPhoto = async (id) => {\n    const { data } = await api.getPhoto(id);\n    if (data.status === \"success\") {\n      const photo = data.data.photo;\n\n      setPhotos((photos) => ({\n        ...photos,\n        data: {\n          ...photos.data,\n          [photo._id]: photo,\n        },\n      }));\n      return photo;\n    }\n\n    return null;\n  };\n\n  return (\n    <Context.Provider\n      value={{\n        auth,\n        signIn,\n        refreshToken,\n        photos,\n        getPhoto,\n        setPhotos,\n        reactPhoto,\n      }}\n    >\n      {props.children}\n    </Context.Provider>\n  );\n}\n\nexport default Context;\n","/Volumes/Data/Self/Coding/unsplash-clone/client/src/components/Header.jsx",[],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/index.js",[],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/pages/index.js",[],["173","174"],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/templates/index.js",[],["175","176"],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/utils/cookieHelper.js",[],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/utils/data.util.js",[],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/templates/PublicRoute.jsx",[],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/templates/PrivateRoute.jsx",[],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/templates/PrivateFluid.jsx",[],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/pages/Home/index.jsx",["177","178"],"/Volumes/Data/Self/Coding/unsplash-clone/client/src/component